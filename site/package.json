{
  "name": "strottos-blog",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && markdownlint-cli2 **/*.mdx #node_modules",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "pages:watch": "npx @cloudflare/next-on-pages --watch",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-date=2023-12-18 --compatibility-flag=nodejs_compat"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@next/mdx": "^14.0.4",
    "@types/mdx": "^2.0.10",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "react-responsive": "^9.0.2"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.8.2",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-next-on-pages": "^1.8.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "markdownlint-cli2": "^0.11.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "vercel": "^33.0.1"
  },
  "markdownlint-cli2": {
    "config": {
      "MD032": false,
      "no-multiple-blanks": false
    }
  }
}
